        "clean": "rimraf ./dist"
    },
    "devDependencies": {
        "@types/node": "^20.14.2",
        "lodash": "^4.17.21",
        "patch-package": "^8.0.0",
        "rimraf": "^5.0.7",
        "swagger-typescript-api": "13.0.9",
        "tsx": "^4.16.2",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "@ton/core": ">=0.60.1"
    },
    "dependencies": {
        "core-js-pure": "^3.38.0"
    },
    "packageManager": "npm@10.9.0"
